pipeline
{
 {
	agent any
	stages
	{
		stage ('SCM checkout')
		{
			git 'https://github.com/Wasim-devops/july-devops.git'
		}
	}
 }
	stage('build && Sonarqube analysis')
	{
		steps 
		{
			withSonarQubeEnv('sonar')
				{
					withMaven(maven:'localMaven') 
						{
							sh'mvn clean package sonar:sonar'
						}
				}
		}
	}
}
